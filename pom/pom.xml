<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.jresearch</groupId>
		<artifactId>org.jresearch.pom</artifactId>
		<version>29-SNAPSHOT</version>
	</parent>
	<groupId>org.jresearch.gavka</groupId>
	<artifactId>org.jresearch.gavka.pom</artifactId>
	<version>1.0.14-SNAPSHOT</version>
	<packaging>pom</packaging>
	<name>JRS kafka UI: Apache Kafka admin application - project POM</name>
	<url>https://bitbucket.org/foal/open-gavka</url>
	<scm>
		<connection>scm:hg:https://foal@bitbucket.org/foal/open-gavka</connection>
		<developerConnection>scm:hg:ssh://hg@bitbucket.org/foal/open-gavka</developerConnection>
		<url>https://bitbucket.org/foal/open-gavka</url>
		<tag>HEAD</tag>
	</scm>
	<modules>
		<module>../api.boot</module>
		<module>../api.app</module>
		<module>../gwt.app</module>
		<module>../md.app</module>
		<module>../gwt.md</module>
		<module>../gwt.core</module>
		<module>../backend</module>
		<module>../domain</module>
		<module>../rest.api</module>
	</modules>
	<properties>
		<!-- Used by the project -->
		<gwtCommons.version>1.0.189</gwtCommons.version>
		<gwtBase.version>1.0.197</gwtBase.version>
		<domino.version>1.0.9-SNAPSHOT</domino.version>
		<gwtCldr.version>1.0.5-SNAPSHOT</gwtCldr.version>
		<gwtTimer.version>1.4.2-SNAPSHOT</gwtTimer.version>
	</properties>
	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>org.jresearch.gwtproject.timer</groupId>
				<artifactId>gwt-timer</artifactId>
				<version>${gwtTimer.version}</version>
			</dependency>
			<dependency>
				<groupId>org.jresearch.commons.gwt</groupId>
				<artifactId>org.jresearch.commons.gwt.bom</artifactId>
				<version>${gwtCommons.version}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
			<dependency>
				<groupId>org.jresearch.commons.gwt.base</groupId>
				<artifactId>org.jresearch.commons.gwt.base.bom</artifactId>
				<version>${gwtBase.version}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
			<dependency>
				<groupId>org.jresearch.gwtproject.i18n</groupId>
				<artifactId>gwt-cldr</artifactId>
				<version>${gwtCldr.version}</version>
			</dependency>
			<dependency>
				<groupId>org.jresearch.dominokit</groupId>
				<artifactId>domino-ui</artifactId>
				<version>${domino.version}</version>
			</dependency>
			<dependency>
				<groupId>org.jresearch.gavka</groupId>
				<artifactId>org.jresearch.gavka.gwt.core</artifactId>
				<type>war</type>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>org.jresearch.gavka</groupId>
				<artifactId>org.jresearch.gavka.gwt.md</artifactId>
				<type>war</type>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>org.jresearch.gavka</groupId>
				<artifactId>org.jresearch.gavka.backend</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>org.jresearch.gavka</groupId>
				<artifactId>org.jresearch.gavka.domain</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>org.jresearch.gavka</groupId>
				<artifactId>org.jresearch.gavka.rest.api</artifactId>
				<version>${project.version}</version>
			</dependency>
		</dependencies>
	</dependencyManagement>
</project>
