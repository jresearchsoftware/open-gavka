<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <!-- <ui:style> .label { margin-left: 5%; margin-top: 5% } </ui:style> -->
    <g:DockLayoutPanel unit="EM" styleName="DockLayoutPanel">
        <!-- DataGrid. -->
        <g:center>
            <g:DeckLayoutPanel ui:field="moduleContainer" styleName="center"/>
        </g:center>

        <!-- Navigation. -->
        <g:north size="2">
            <g:HorizontalPanel ui:field="navBar" styleName="Navigation" spacing="10"/>
        </g:north>
    </g:DockLayoutPanel>
</ui:UiBinder>